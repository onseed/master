section.aws_box .run_sample{padding:15px 20px 10px 20px;}


h3.label{display:block;  background:#f9f8f8; padding:0px 10px; font-size:11px; height:20px; line-height:20px; position:relative; font-weight:normal; margin-top:15px; cursor:pointer; border-radius:10px;
	border:1px solid #f9f9f9;}	
h3.label strong{font-weight:bold;}

h3.label:after{display:block; content:'+'; position:absolute;top:4px; right:10px; font-size:0px; line-height:0px; background-image:url("../img/icn_etc.svg");
	width:8px; height:8px; background-position:-57px -108px; background-size:auto 225px;}
h3.label.open::after{content: '-'; background-position:-76px -108px; }
h3.label.open{border-radius:10px 10px 0  0;}

section.aws_box .play_box strong{color:#000;}

section.aws_box button.btn_play,
section.aws_box .play_box button:not(.clone),
.modal_cont button{border:1px solid #d9d9d9; background:#fff; border-radius:16px; padding:8px 15px; box-shadow: 0px 1px 5px rgba(180,180,180,0.2); font-size:14px; margin:20px 5px 0 0; display:inline-block; text-align:left;}
.modal_cont button{margin:0;}	
section.aws_box .play_box button .option_txt {background: none; box-shadow: none; overflow-x: none; width: auto; box-sizing: border-box; white-space: nowrap; border:none; padding: 0;}
section.aws_box .play_box .modal_cont button{margin:0;}

section.aws_box .normal code[class*="language-"],
section.aws_box .play_box button code[class*="language-"], 
section.aws_box .play_box button pre[class*="language-"]{background:none; font-size:14px;}

section.aws_box .code-toolbar{display:none;}
section.aws_box .option_txt .code-toolbar{display:block;}
section.aws_box .option_txt code[class*="language-"], section.aws_box .option_txt pre[class*="language-"]{ background:none;}

.bdbot{border-bottom:1px solid #f2f2f2; padding-bottom:20px;}
.bdtop{border-top:1px solid #f2f2f2; padding-top:20px;}

@media screen and (max-width:1100px) {    
    .subnav{padding: 60px 10px 10px;}
    .contents{padding:60px 10px;}
    .contents .view_sort{top:70px; right:15px;}        
	section.aws_box {margin: 50px 0px 0px; padding: 5px; }    
	header.ui nav a {padding: 0 12px;}	
	.ui_guideline {padding-left:10px;}
}


/************************* PrismJS 1.15.0 ********************************/
code[class*="language-"],
pre[class*="language-"] { color: black; background: #f9f8f8; text-align: left; white-space: pre-wrap; word-spacing: normal; font-size: 12px; font-family: '맑은 고딕'; letter-spacing:-0.01em;
    word-break: normal; word-wrap: normal; line-height: 1.7; /*-moz-tab-size: 0; -o-tab-size: 0;*/ tab-size: 8px; -webkit-hyphens: none; -moz-hyphens: none; -ms-hyphens: none; hyphens: none;}
pre[class*="language-"]::-moz-selection, pre[class*="language-"] ::-moz-selection,
code[class*="language-"]::-moz-selection, code[class*="language-"] ::-moz-selection { text-shadow: none; background: #b3d4fc; }
/* Code blocks */
pre[class*="language-"] { padding: 10px; margin:0; overflow: auto; box-sizing: border-box;}
/*:not(pre) > code[class*="language-"],
pre[class*="language-"] {}*/
.option_txt pre[class*="language-"] {padding: 0px; margin:0px;}

/* Inline code */
.token.comment,
.token.prolog,
.token.doctype,
.token.cdata {color: slategray;}
.token.punctuation {color: #999;}
.namespace {opacity: .7;}
.token.property,
.token.tag,
.token.boolean,
.token.number,
.token.constant,
.token.symbol,
.token.deleted {color: #905;}
.token.selector,
.token.attr-name,
.token.string,
.token.char,
.token.builtin,
.token.inserted {color: #690;}
.token.operator,
.token.entity,
.token.url,
.language-css .token.string,
.style .token.string { color: #9a6e3a; /*background: hsla(0, 0%, 100%, .5);*/}
.token.atrule,
.token.attr-value,
.token.keyword {color: #07a;}
.token.function,
.token.class-name {color: #DD4A68;}
.token.regex,
.token.important,
.token.variable {color: #e90;}
.token.important,
.token.bold {font-weight: bold;}
.token.italic {font-style: italic;}
.token.entity {cursor: help;}


div.code-toolbar > .toolbar a,
div.code-toolbar > .toolbar button,
div.code-toolbar > .toolbar span {
    color: #bbb;
    font-size: .8em;
    padding: 0 .5em;
    background: #f5f2f0;
    background: rgba(224, 224, 224, 0.2);
    box-shadow: 0 2px 0 0 rgba(0,0,0,0.2);
    border-radius: .5em;
}

div.code-toolbar > .toolbar a:hover,
div.code-toolbar > .toolbar a:focus,
div.code-toolbar > .toolbar button:hover,
div.code-toolbar > .toolbar button:focus,
div.code-toolbar > .toolbar span:hover,
div.code-toolbar > .toolbar span:focus {
    color: inherit;
    text-decoration: none;
}

