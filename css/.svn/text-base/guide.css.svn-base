/************************* Guide ********************************/
header.ui { position:absolute;  width:100%; /*top:0;*/ left:0; height:65px; line-height:65px; z-index:1;}

header.ui:after{display:block; content:''; clear:both;}
header.ui .logo{display:inline-block; margin-left:20px;}
header.ui h1 img{display:inline-block; width:50px; vertical-align:middle; margin:-5px 0px 0 0; }
.mobile header.ui h1 img{width:40px;}

header.ui .nav_lay{position:relative; height:65px;}
header.ui .nav_lay .current_bar{position:absolute; width:100px; height:2px; border-radius:2px; background:#999; bottom:10px; display:none;}
.mobile header.ui .nav_lay .current_bar{bottom:20px;}


.desktop header.ui{min-width:495px;}

header.ui h1{display:block; float:left; font-size: 16px; font-weight:normal; word-spacing: -2px; color:#111; 
height: 40px; 
    line-height: 40px;
    border-radius: 16px;
    margin-top:15px;
}
   

/************   loading   ***********/
.start_dim{
	display:block;
	width:100%;
    height:100%;
    top:0;
    left:0;
    position:fixed;
    z-index:10000;    
    /*pointer-events:none; */
    background:#fff;
    /*opacity:0.7;*/
}

/*.new_mopism .start_dim{background:#e3eaf2;}*/
.new_mopism .start_dim{background:#f7f7f7;}

.start_dim span{display:block; content:''; position:absolute;  width:70px; height:70px;  margin:0 0 0 -35px; top:50%; left:50%;
 background:url('../img/logo.svg') no-repeat 0 0; background-size:100%; animation:upndownload 0.7s infinite; 
 z-index:1000001;
}
@keyframes upndownload{
	0% {margin-top:-50px;}
	60% {margin-top:-60px;}
	100% {margin-top:-50px;}
}
.start_dim:after{
	display:block; content:'';  position:fixed; height: 10px; width: 40px; margin:15px 0 0 -20px; left:50%; top:50%;
	background:rgba(0,0,0, 0.1); border-radius:50%;
	animation: outer 0.7s infinite;
}
.start_dim.loaded span{animation:loaded 0.5s forwards;}
.start_dim.loaded:after{
	animation: loaded_shdow 0.5s forwards;
}
@keyframes outer{
	0% {transform: scale(1);}
	60% {transform: scale(0.8);}
	100% {transform: scale(1);}
}
@keyframes loaded{
	0% {margin-top:0px;}
	100% {margin-top:-10%;}
}
@keyframes loaded_shdow{
	0% {transform: scale(1);}
	100% {transform: scale(0);}
}
 
