header.ui nav{float:right; font-weight:bold; font-size:0;  margin-right:20px;}
header.ui nav a{display:inline-block; font-size:13px; padding:0 20px; position:relative; color:#999; margin: 15px 5px; height: 40px;
    line-height: 40px; border-radius: 20px;}
header.ui nav a.current{color:#000;}
header.ui.bottom nav a.current:after{height:2px;}
header.ui nav a:hover{color:#000; animation:animated_hover 0.5s}
header.ui.bottom nav a.current:after{display:block; content:''; position:absolute; width:100%; height:2px; background:#333; bottom:0px; left:0;}
header.ui.bottom  .current_bar{margin-bottom:-10px;}

header.ui.bottom{position:fixed; top:unset; bottom:0;  height:35px; padding-top:10px; padding-bottom:10px; line-height:35px; background:#fff; animation:animated_bottom 0.5s; z-index:10;}
header.ui.bottom h1{font-size:12px; padding:0px; margin-left:0px; color:#000; margin-top:5px;}
header.ui.bottom h1 img{width:40px; margin:-7px 0 0 0;}
header.ui.bottom nav a{font-size:12px; padding:0 10px; margin:0 5px;}

header.ui.bottom .nav_lay{height:30px; display:block; margin-right:20px;}
header.ui.bottom .nav_lay .current_bar{bottom:-10px;}
.mobile header.ui.bottom{ padding-top:10px;}
.mobile header.ui.bottom .nav_lay{ margin-right:0px;}

