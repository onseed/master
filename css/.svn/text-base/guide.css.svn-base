/* default */
body { background-color:#fff; color:#111; font-size:14px; line-height:1.4; font-family: '맑은 고딕'; letter-spacing:-0.01em;
    -webkit-font-smoothing:antialiased; -webkit-text-size-adjust:none; -moz-osx-font-smoothing:grayscale; word-break:keep-all;}
    
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,fieldset,p,button,iframe,th,td { margin:0; padding:0; }
article,aside,details,figcaption,figure,footer,header,hgroup,nav,section,summary { display:block; margin:0; padding:0; }
h1,h2,h3,h4,h5,h6,dt { font-weight:normal;}
img,fieldset,iframe,button { border:0 none; }
li { list-style:none; }
button { border:0 none; background:none;}
button:focus{}
table {  border-collapse:collapse; border-spacing:0;}
.mobile table { table-layout:fixed;}
th, td { border-collapse:collapse; }
legend, caption, hr { display:none; }
i, em, u, cite { font-style:normal; }
a { color:#4b4c4c; text-decoration:none; /*outline-offset:-1px; */}
.chrome a, .safari a {/* outline-offset:-3px; */}
body, html{width:100%; height:100%}
html { overflow-y:scroll; }
html.dotcom { overflow-y:inherit; }
html.no-scroll { overflow:hidden; }
body:before { position:fixed; left:0; top:0; width:100%; height:0; content:''; }/* fix webkit bug(scroll event delay) */
img { width:100%; }
dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,fieldset,p,button,iframe,th,td,strong,span { line-height:inherit; }

.option_txt{background:#f9f8f8; line-height:1.8em; 
    overflow-x: auto; width: 100%; box-sizing: border-box; white-space: nowrap; border-radius:16px; padding:10px 20px;}
.option_txt:after{display:block; content:''; clear:both; }

.icn_top{display:block; position:fixed; bottom:-35px; right:10px; padding:7px; border-radius:50%; z-index:10;  box-shadow: 0px 1px 3px rgba(180,180,180,1.0); background:#fff;}
.icn_top .icn{display:block;  background-size:auto 450px;
	background-position: -307px -216px; width: 17px; height: 17px;}	
.icn_top:hover{background:#f8f7f7; box-shadow:none;} 	

.mgt_ss{margin-top:5px !important;}
.mgt_s{margin-top:10px !important;}
.mgt_m{margin-top:15px !important;}
.mgt_l{margin-top:20px !important;}

.mgb_ss{margin-bottom:5px !important;}
.mgb_s{margin-bottom:10px !important;}
.mgb_m{margin-bottom:15px !important;}
.mgb_l{margin-bottom:20px !important;}

.bd_top{border-top:1px solid #f8f8f8; padding:20px 0 0 ; margin:20px 0 0 ;}

.block {display:block !important;}
.hide {display:none !important;}

.center{text-align:center !important;}

.point{color:#293eba !important;}
button.point {
    color: inherit !important;
    border-color: #111 !important;
}

.clear:after{display:block; content:' '; clear:both;}

